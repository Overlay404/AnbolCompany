<Window x:Class="AnbolCompany.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AnbolCompany"
        mc:Ignorable="d"
        Title="AnbolCompany" Height="450" Width="800">
    <Grid Style="{DynamicResource StyleGrid}">
        <Grid.RowDefinitions>
            <RowDefinition Height="49*"/>
            <RowDefinition Height="365*"/>
            <RowDefinition Height="20*"/>
        </Grid.RowDefinitions>
        <Image HorizontalAlignment="Right" VerticalAlignment="Top" Panel.ZIndex="1" Width="20" Height="20" Margin="0 10 10 0" MouseUp="Image_MouseUp"/>
        <Frame x:Name="frame" Grid.Row="1"/>
    </Grid>
</Window>
